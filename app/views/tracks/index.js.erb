$(function () {

  // Change 'enqueue' link to 'enqueueing...' while the enqueueing is in progress.
  $('.enqueue').click(function () {
    this['previousText'] = $(this).html()
    $(this).html('enqueueing...')
  }).bind('ajax:success', function () {
    $(this).html(this['previousText'] || 'enqueue')
  })

})
